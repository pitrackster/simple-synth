{
  "name": "simple-synth",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": { }
    },
    "extends": "react-app",
    "rules": {
      "indent": [2, 2, { "SwitchCase": 1 }],
      "linebreak-style": [2, "unix"],
      "quotes": [2, "single"],
      "semi": [2, "never"],
      "space-before-function-paren": ["error", {
        "anonymous": "always",
        "named": "never"
      }]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
